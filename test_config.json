{
  "Middleware": ["logger"],
  "Routes": [
    {
      "Route": "/info",
      "Id": "info",
      "Resource": {
        "ResourceType": "static_file",
        "Params": {
          "Filepath": "README.md"
        }
      }
    },
    {
      "Route": "/design",
      "Id": "design",
      "Resource": {
        "ResourceType": "static_file",
        "Params": {
          "Filepath": "design.md"
        }
      }
    },
    {
      "Route": "/code",
      "Id": "resources",
      "Resource": {
        "ResourceType": "directory",
        "Params": {
          "Path": "resources",
          "Whitelist": ["*"],
          "AllowDirectoryBrowsing": false
        }
      }
    },
    {
      "Route": "/update",
      "Id": "update",
      "Resource": {
        "ResourceType": "update_router",
        "Params": {}
      }
    },
    {
      "Route": "/tak/*path",
      "Id": "tak",
      "Resource": {
        "ResourceType": "proxy",
        "Params": {
          "Host": "http://localhost:3000",
          "Path": "/tak/*path",
          "Methods": ["GET"]
        }
      }
    }
  ]
}
