{
  "LastUpdated": 100,
  "Middleware": ["logger"],
  "Routes": [
    {
      "Route": "/info",
      "Id": "info",
      "Resource": {
        "ResourceType": "static_file",
        "Params": { "Filepath": "README.md" }
      }
    },
    {
      "Route": "/design",
      "Id": "design",
      "Resource": {
        "ResourceType": "static_file",
        "Params": { "Filepath": "design.md" }
      }
    },
    {
      "Route": "/code",
      "Id": "resources",
      "Resource": {
        "ResourceType": "directory",
        "Params": {
          "AllowDirectoryBrowsing": false,
          "Path": "resources",
          "Whitelist": ["*"]
        }
      }
    },
    {
      "Route": "/api",
      "Id": "api",
      "Resource": { "ResourceType": "api", "Params": {} }
    },
    {
      "Route": "/tak/*path",
      "Id": "tak",
      "Resource": {
        "ResourceType": "proxy",
        "Params": {
          "Host": "http://localhost:3000",
          "Methods": ["GET"],
          "Path": "/tak/*path"
        }
      }
    }
  ]
}
