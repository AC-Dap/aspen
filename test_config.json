{
  "LastUpdated": 100,
  "Middleware": ["logger"],
  "Routes": [
    {
      "Route": "/info",
      "Id": "info",
      "Resource": {
        "ResourceType": "static_file",
        "Params": { "Filepath": "README.md" }
      }
    },
    {
      "Route": "/design",
      "Id": "design",
      "Resource": {
        "ResourceType": "static_file",
        "Params": { "Filepath": "design.md" }
      }
    },
    {
      "Route": "/code",
      "Id": "resources",
      "Resource": {
        "ResourceType": "directory",
        "Params": {
          "AllowDirectoryBrowsing": true,
          "Path": "services/test",
          "Whitelist": ["*"]
        }
      }
    },
    {
      "Route": "/test/*path",
      "Id": "test-resource",
      "Resource": {
        "ResourceType": "proxy",
        "Params": {
          "Host": "http://localhost",
          "Methods": ["GET"],
          "Path": "/*path"
        }
      }
    },
    {
      "Route": "/api",
      "Id": "api",
      "Resource": { "ResourceType": "api", "Params": {} }
    },
    {
      "Route": "/tak/*path",
      "Id": "tak",
      "Resource": {
        "ResourceType": "proxy",
        "Params": {
          "Host": "http://localhost:3000",
          "Methods": ["GET"],
          "Path": "/tak/*path"
        }
      }
    }
  ],
  "Services": [
    {
      "Id": "test",
      "Remote": "https://github.com/docker/getting-started-todo-app.git",
      "CommitHash": "42585d57bbbbe3f576dae92c8d57c5761a6bec7a"
    }
  ]
}
