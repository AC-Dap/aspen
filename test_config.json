[
  {
    "Route": "/info",
    "Resource": {
      "ResourceType": "static_file",
      "Params": ["info", "README.md"]
    }
  },
  {
    "Route": "/design",
    "Resource": {
      "ResourceType": "static_file",
      "Params": ["design", "design.md"]
    }
  },
  {
    "Route": "/code",
    "Resource": {
      "ResourceType": "directory",
      "Params": ["resources", "resources", ["*"], false]
    }
  },
  {
    "Route": "/update",
    "Resource": {
      "ResourceType": "update_router",
      "Params": ["update"]
    }
  },
  {
    "Route": "/tak/*path",
    "Resource": {
      "ResourceType": "proxy",
      "Params": ["tak", "http://localhost:3000", "/tak/*path", ["GET"]]
    }
  }
]
